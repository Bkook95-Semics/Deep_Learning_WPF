<Window x:Class="Deep_WPF.Labeling"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Deep_WPF"
        mc:Ignorable="d"
        Title="Labeling" Height="300" Width="300">
    <StackPanel>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" Margin="10" Name="btn_opencv">OPENCV</Button>
            <Button Grid.Column="1" Margin="10" Name="btn_tool" Click="btn_tool_Click">TOOL</Button>
        </Grid>
    </StackPanel>
</Window>
